{
  "name": "threshold-gulpify-bb",
  "version": "0.0.1",
  "description": "The all-encompassing starting point for my front end dev projects using Gulp and Browserify with Backbone.",
  "author": "Matt Przybylski <matt@reintroducing.com>",
  "main": "js/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/reintroducing/threshold-gulpify-bb.git"
  },
  "keywords": [
    "matt przybylski",
    "threshold",
    "gulp",
    "html",
    "css",
    "js",
    "javascript",
    "front end",
    "backbone",
    "underscore"
  ],
  "browserify": {
    "transform": [
      "browserify-shim",
      "node-underscorify"
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "underscore": "./node_modules/lodash/dist/lodash.underscore.js",
    "backbone": "./node_modules/backbone/backbone.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "backbone": {
      "depends": [
        "jquery",
        "underscore:_"
      ],
      "exports": "Backbone"
    }
  },
  "devDependencies": {
    "backbone": "latest",
    "backbone-relational": "latest",
    "lodash": "latest",
    "node-underscorify": "latest",
    "browser-sync": "latest",
    "browserify": "latest",
    "browserify-shim": "latest",
    "del": "latest",
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-compass": "latest",
    "gulp-if": "latest",
    "gulp-jshint": "latest",
    "gulp-notify": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-uglify": "latest",
    "gulp-usemin": "latest",
    "gulp-util": "latest",
    "jquery": "latest",
    "jshint-stylish": "latest",
    "require-dir": "latest",
    "run-sequence": "latest",
    "vinyl-buffer": "latest",
    "vinyl-source-stream": "latest",
    "watchify": "latest"
  }
}
